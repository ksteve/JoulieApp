<resources>

    <style name="MyLockTheme" parent="Lock.Theme">
        <item name="Auth0.HeaderLogo">@mipmap/ic_joulie_logo</item>
        <item name="Auth0.HeaderTitle">@string/app_name</item>
        <item name="Auth0.HeaderTitleColor">@color/colorPrimary</item>
        <item name="Auth0.HeaderBackground">@color/com_auth0_lock_header_background</item>
        <item name="Auth0.PrimaryColor">@color/colorPrimaryDark</item>
        <item name="Auth0.DarkPrimaryColor">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
</resources>
